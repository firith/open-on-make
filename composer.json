{
    "name": "ahuggins/open-on-make",
    "description": "Automatically open files in editor when Artisan make:* is executed",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Huggins",
            "email": "andrewhuggins@gmail.com"
        }
    ],
    "require": {
        "php" : "~7.2",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|~6.0|~7.0",
        "laravel/helpers": "^1.1",
        "symfony/finder": "~3.3|^4.0|^4.3",
        "imliam/laravel-env-set-command": "^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "OpenOnMake\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OpenOnMake\\Providers\\OpenOnMakeServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.4"
    }
}
